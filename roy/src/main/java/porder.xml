<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="porderns">
	<select id="select" parameterType="string" resultType="porder">
		select * from porder where id=#{m_id}
	</select>
	<insert id="insert" parameterType="porder">
	<selectKey keyProperty="o_num" resultType="integer" order="BEFORE">
		select O_NUM_SEQ.nextval FROM DUAL
	</selectKey>
		insert into porder(o_num,o_date,o_req,p_num,m_id,o_qty,o_cost) 
		values (#{o_num},sysdate,#{o_req},#{p_num},#{m_id},#{o_qty},#{o_cost})
	</insert>
</mapper>